location.name}</Popup>\r\n                </Marker>\r\n            ))}\r\n        </MapContainer>\r\n    )\r\n}\r\n\r\nexport default MinecraftMap;","C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\context\\LayerContext.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\api\\testData.ts",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\DeleteButton.tsx",[],["58","59"],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\LocationList\\LocationsList.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\LocationList\\LocationListItem.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\LocationList\\CoordinateLabel.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\Sidebar.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\index.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\App.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\Location\\AddLocation.tsx",["60","61"],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\Location\\CoordinateControl.tsx",[],"C:\\Source\\robgray\\MinecraftMapper\\front\\src\\components\\Location\\Categories.tsx",[],{"ruleId":"62","severity":1,"message":"63","line":1,"column":22,"nodeType":"64","messageId":"65","endLine":1,"endColumn":31},{"ruleId":"62","severity":1,"message":"66","line":1,"column":33,"nodeType":"64","messageId":"65","endLine":1,"endColumn":41},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"62","severity":1,"message":"71","line":1,"column":61,"nodeType":"64","messageId":"65","endLine":1,"endColumn":65},{"ruleId":"62","severity":1,"message":"72","line":25,"column":11,"nodeType":"64","messageId":"65","endLine":25,"endColumn":32},"@typescript-eslint/no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.","no-native-reassign",["73"],"no-negated-in-lhs",["74"],"'Text' is defined bu